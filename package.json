{
  "name": "graffy",
  "version": "0.0.1",
  "description": "Graffy Project",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindet/graffy.git"
  },
  "scripts": {
    "lint": "eslint scripts src",
    "test": "jest",
    "package": "./scripts/package",
    "example:server": "node --experimental-modules --es-module-specifier-resolution=node example/server.js",
    "example:client": "rollup -c example/rollup.config.js",
    "example": "yarn example:client && yarn example:server",
    "site": "rollup -w -c site/rollup.config.js",
    "format": "prettier --write '{src,scripts,docs}/**/*.{js,css,html,json,md}' && eslint --fix src",
    "next": "next",
    "next:build": "next build",
    "next:start": "next start",
    "next:static": "next build && next export -o docs"
  },
  "workspaces": [
    "src/*",
    "example"
  ],
  "author": "aravindet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aravindet/graffy/issues"
  },
  "homepage": "https://graffy.js.org",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@mdx-js/mdx": "^1.5.1",
    "@zeit/next-mdx": "^1.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "globby": "^10.0.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "merge-async-iterators": "^0.2.1",
    "next": "^9.1.2-canary.2",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-analyzer": "^3.2.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "text-encoder-lite": "^2.0.0",
    "yargs": "^14.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
