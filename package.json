{
  "name": "graffy",
  "version": "0.0.2",
  "description": "Graffy Project",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindet/graffy.git"
  },
  "scripts": {
    "lint": "eslint scripts src pages",
    "test": "jest",
    "package": "./scripts/package",
    "example:server": "node --experimental-modules --es-module-specifier-resolution=node example/server.js",
    "example:client": "rollup -c example/rollup.config.js",
    "example": "yarn example:client && yarn example:server",
    "format": "prettier --write '{src,scripts,example,pages}/**/*.{js,css,html,json,md}' && eslint --fix scripts src pages",
    "next": "next",
    "next:build": "next build",
    "next:start": "next start",
    "next:static": "next build && next export",
    "build": "yarn next:build",
    "start": "yarn next:start -p $PORT"
  },
  "workspaces": [
    "src/*",
    "example"
  ],
  "author": "aravindet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aravindet/graffy/issues"
  },
  "homepage": "https://graffy.js.org",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.2",
    "@mapbox/rehype-prism": "^0.4.0",
    "@mdx-js/mdx": "^1.5.8",
    "@rollup/plugin-commonjs": "x",
    "@rollup/plugin-node-resolve": "x",
    "@rollup/plugin-replace": "x",
    "@zeit/next-mdx": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "globby": "^11.0.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "jest": "^25.3.0",
    "lodash": "^4.17.15",
    "merge-async-iterators": "^0.2.1",
    "nanoid": "^3.1.3",
    "next": "9.2.x",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-json-view": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-babel": "^4.4.0",
    "text-encoder-lite": "^2.0.0",
    "ws": "^7.2.3",
    "yargs": "^15.3.1"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
