{
  "name": "graffy",
  "version": "1.0.0",
  "description": "Graffy Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindet/graffy.git"
  },
  "scripts": {
    "test": "jest",
    "build-pkg": "NODE_ENV=production rollup -c",
    "server": "node examples/server",
    "client": "cd examples/client && SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "example": "(yarn client &) && yarn server",
    "format": "prettier --write '{src,scripts,docs}/**/*.{js,css,html,json,md}' && eslint --fix src"
  },
  "workspaces": [
    "src/*",
    "examples/*"
  ],
  "private": true,
  "author": "aravindet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aravindet/graffy/issues"
  },
  "homepage": "https://github.com/aravindet/graffy#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "babel-plugin-async-to-promises": "^1.0.5",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "globby": "^9.0.0",
    "jest": "^24.3.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rollup": "^1.6.0",
    "rollup-plugin-analyzer": "^3.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0"
  }
}
